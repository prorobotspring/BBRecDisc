		<servicio id="desconexion.rim.roboextravio">
			<annotation>
				<documentation>
					Comando para la desconexion de un suscriptor de RIM por cambio de estado a Robo y/o Extravio
				</documentation>
			</annotation>
			<comandos>
				<!-- <comando delegador="VerificarStatusRIM" servicio="desconexion.rim.roboextravio">
					<parametros>
					<parametro formal="MSISDN" actual="MSISDN" />
					<parametro formal="IMSI" actual="IMSI" />
					<parametro formal="Status" actual="Status" />
					<parametro formal="CosName" actual="CosName" />
					<parametro formal="Accion" constante="D" />
					<parametro formal="Canal" constante="N" />
					</parametros>
					</comando> -->
				<comando delegador="RegistrarAccionRIMBD" servicio="desconexion.rim.roboextravio">
					<parametros ambito="global">
						<parametro formal="Accion" constante="D" />
						<parametro formal="Canal" constante="N" />
						<parametro formal="Estado" constante="0" />
						<parametro formal="Descripcion" constante="In Process" />
					</parametros>
				</comando>
				<comando delegador="DesconectarRIMRoboExtravio" servicio="desconexion.rim.roboextravio">
					<parametros ambito="global">
						<!-- <parametro formal="SERVICE_TYPE" constante="CANCEL" /> -->
					</parametros>
					<retorno ignorar="y" codes="*" />
				</comando>
				<comando delegador="ActualizarStatusErrorIL" servicio="desconexion.rim.roboextravio">
					<condiciones>
						<comando delegador="local" servicio="cond:in">
							<parametros ambito="local">
								<parametro formal="lvalor" actual="_rc_resultado" />
								<parametro formal="rvalor" constante="0" />
								<parametro formal="negado" constante="true" type="java.lang.Boolean" />
							</parametros>
						</comando>
					</condiciones>
					<parametros ambito="global">
						<parametro formal="Accion" constante="D" />
						<parametro formal="Canal" constante="N" />
						<parametro formal="MSISDN" actual="MSISDN" />
						<parametro formal="IMSI" actual="IMSI" />
						<parametro formal="Estado" actual="_rc_resultado" />
						<parametro formal="Descripcion" actual="_rc_razon" />
					</parametros>
				</comando>
			</comandos>
		</servicio>

		<servicio id="validar.tipo.reconexion">
			<annotation>
				<documentation>Comando para la validacion del tipo de reconexion a realizar</documentation>
			</annotation>
			<comandos>
				<comando delegador="PreviousStateValidate" servicio="validar.tipo.reconexion">
					<parametros>
						<parametro formal="MSISDN" actual="MSISDN" />
						<parametro formal="IMSI" actual="IMSI" />
						<parametro formal="Status" actual="Status" />
						<parametro formal="CosName" actual="CosName" />
					</parametros>
					<retorno ignorar="y" codes="*" />
				</comando>
				<comando delegador="local" servicio="stm:setAll">
					<parametros ambito="local">
						<parametro formal="nameList" constante="_rc_resultado_validate _rc_razon_validate" />
						<parametro formal="rvalor_1" actual="_rc_resultado" />
						<parametro formal="rvalor_2" actual="_rc_razon" />
					</parametros>
					<retorno ignorar="y" codes="*" />
				</comando>
				<!-- Si el resultado es cero, el previousState es Robo y/o extravio -->
				<!--<comando delegador="EncolarSolicitudReconexion" servicio="reconexion.rim.roboextravio">
					<condiciones>
						<comando delegador="local" servicio="cond:in">
							<parametros ambito="local">
								<parametro formal="lvalor" actual="_rc_resultado_validate" />
								<parametro formal="rvalor" constante="0" />
							</parametros>
						</comando>
					</condiciones>
					<parametros ambito="global"></parametros>
				</comando>-->
				<!-- Si el resultado es uno, el previousState no es Robo y/o extravio -->
				<comando delegador="EncolarSolicitudReconexion" servicio="reconexion.rim.recarga">
					<condiciones>
						<comando delegador="local" servicio="cond:in">
							<parametros ambito="local">
								<parametro formal="lvalor" actual="_rc_resultado_validate" />
								<parametro formal="rvalor" constante="1" />
							</parametros>
						</comando>
					</condiciones>
					<parametros ambito="global"></parametros>
				</comando>
			</comandos>
		</servicio>

		<servicio id="reconexion.rim.roboextravio">
			<annotation>
				<documentation>
					Comando para la reconexion de un suscriptor en RIM por cambio de estado: Activo proveniente de Robo
					y/o Extravio
				</documentation>
			</annotation>
			<comandos>
				<comando delegador="RegistrarAccionRIMBD" servicio="reconexion.rim.roboextravio">
					<parametros ambito="global">
						<parametro formal="Accion" constante="R" />
						<parametro formal="Canal" constante="N" />
						<parametro formal="Estado" constante="0" />
						<parametro formal="Descripcion" constante="In Process" />
					</parametros>
				</comando>
				<comando delegador="ConectarRIMRoboExtravio" servicio="reconexion.rim.roboextravio">
					<condiciones>
						<comando delegador="local" servicio="cond:inSplit">
							<parametros ambito="local">
								<parametro formal="lvalor" actual="CosName" />
								<parametro formal="rvalor"
									constante="BlackBerry 50MB ZF;BlackBerry Ilimitado;BlackBerry 50MB;BlackBerry Ilimitado ZF" />
							</parametros>
						</comando>
					</condiciones>
					<parametros ambito="global">
						<parametro formal="SERVICE_PLAN" constante="Prosumer Prepaid B" />
					</parametros>
					<retorno ignorar="y" codes="*" />
				</comando>
				<comando delegador="ConectarRIMRoboExtravio" servicio="reconexion.rim.roboextravio">
					<condiciones>
						<comando delegador="local" servicio="cond:inSplit">
							<parametros ambito="local">
								<parametro formal="lvalor" actual="CosName" />
								<parametro formal="rvalor" constante="BlackBerry Social;BlackBerry Social ZF" />
							</parametros>
						</comando>
					</condiciones>
					<parametros ambito="global">
						<parametro formal="SERVICE_PLAN" constante="BIS Social Prepaid" />
					</parametros>
					<retorno ignorar="y" codes="*" />
				</comando>
				<comando delegador="ActualizarStatusErrorIL" servicio="desconexion.rim.roboextravio">
					<condiciones>
						<comando delegador="local" servicio="cond:in">
							<parametros ambito="local">
								<parametro formal="lvalor" actual="_rc_resultado" />
								<parametro formal="rvalor" constante="0" />
								<parametro formal="negado" constante="true" type="java.lang.Boolean" />
							</parametros>
						</comando>
					</condiciones>
					<parametros ambito="global">
						<parametro formal="Accion" constante="R" />
						<parametro formal="Canal" constante="N" />
						<parametro formal="MSISDN" actual="MSISDN" />
						<parametro formal="IMSI" actual="IMSI" />
						<parametro formal="Estado" actual="_rc_resultado" />
						<parametro formal="Descripcion" actual="_rc_razon" />
					</parametros>
				</comando>
			</comandos>
		</servicio>

